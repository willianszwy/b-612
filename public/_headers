  Pragma: no-cache
  Expires: 0

# Cache busting para assets
/assets/*
  Cache-Control: public, max-age=31536000, immutable

# Service Worker sempre fresh
/sw-custom.js
  Cache-Control: no-cache, no-store, must-revalidate
  
# Manifest sempre fresh
/manifest.webmanifest
  Cache-Control: no-cache, no-store, must-revalidate